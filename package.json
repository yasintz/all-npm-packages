{
  "version": "0.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-push": "tsdx lint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src$1"
    }
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-root-import",
        {
          "rootPathSuffix": "./src",
          "rootPathPrefix": "~/"
        }
      ]
    ]
  },
  "name": "route-schema",
  "author": "yasintz",
  "module": "dist/route-schema.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yasintz/npm-route-schema"
  },
  "bugs": {
    "url": "https://github.com/yasintz/npm-route-schema/issues"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "babel-plugin-root-import": "^6.4.1",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2"
  }
}
